<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>
<%= form_tag "/login" do  %>
  <%= label_tag :username %>
  <%= text_field_tag :username %> <br>

  <%= label_tag :password_digest %>
  <%= password_field_tag :password_digest %> <br>

  <%= label_tag :confirmation %>
  <%= password_field_tag :password_confirmation %>

  <%= submit_tag "Login" %>
<% end %>